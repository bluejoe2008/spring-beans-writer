package cn.csdb.commons.xmlbeans.node.creator;

import cn.csdb.commons.xmlbeans.node.value.BeanRefNode;
import cn.csdb.commons.xmlbeans.writer.NodeCreatorContext;

public class BeanRefNodeCreator
{
	public BeanRefNode createValueNode(String nodeId, NodeCreatorContext ctx)
	{
		BeanRefNode node = new BeanRefNode(nodeId);
		return node;
	}
}
